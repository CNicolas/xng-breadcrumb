{
  "name": "xng-breadcrumb",
  "version": "0.0.0-development",
  "license": "MIT",
  "description": "A declarative and reactive breadcrumb for Angular 6 and beyond https://www.npmjs.com/package/xng-breadcrumb",
  "author": {
    "name": "Uday Vunnam",
    "email": "uday4vunnam@gmail.com"
  },
  "homepage": "https://github.com/udayvunnam/xng-breadcrumb",
  "bugs": "https://github.com/udayvunnam/xng-breadcrumb/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/udayvunnam/xng-breadcrumb.git"
  },
  "keywords": [
    "angular",
    "angular6",
    "angular2",
    "ng",
    "breadcrumb",
    "ngx"
  ],
  "scripts": {
    "ng": "ng",
    "build:dev": "concurrently 'ng build xng-breadcrumb --watch' 'ng serve' ",
    "postbuild:dev": "npm run copy:readme",
    "build:prod": "ng build xng-breadcrumb",
    "postbuild:prod": "npm run copy:readme",
    "copy:readme": "cp README.md dist/xng-breadcrumb/README.md",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "semantic-release": "semantic-release",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "concurrently": "^4.1.0",
    "conventional-changelog-cli": "^2.0.17",
    "core-js": "^2.5.4",
    "pretty-quick": "^1.10.0",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "xng-breadcrumb": "0.0.1",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-ng-packagr": "~0.13.0",
    "@angular/cli": "~7.3.8",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "commitizen": "^3.1.1",
    "husky": "^2.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "prettier": "1.17.0",
    "protractor": "~5.4.0",
    "semantic-release": "^15.13.3",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "commitlint"
    }
  }
}
